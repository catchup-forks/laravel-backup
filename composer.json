{
    "name": "vinkla/backup",
    "description": "An easy-to-use backup manager for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "backup", "database", "flysystem"],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com",
            "homepage": "https://vinkla.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/console": "5.4.* || 5.5.*",
        "illuminate/contracts": "5.4.* || 5.5.*",
        "illuminate/support": "5.4.* || 5.5.*",
        "zenstruck/backup": "^1.2"
    },
    "require-dev": {
        "graham-campbell/testbench": "^4.0",
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\Backup\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Backup\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        },
        "laravel": {
            "providers": [
                "Vinkla\\Backup\\BackupServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
